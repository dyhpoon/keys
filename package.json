{
  "name": "@dyhpoon/keys",
  "version": "1.0.0",
  "main": "dist/keys-cjs.js",
  "module": "dist/keys-es.js",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "prepush": "npm run te && npm run lint"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^22.1.0",
    "@types/node": "^9.4.0",
    "eslint": "^4.16.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.5.0",
    "jest": "^22.1.4",
    "prettier": "^1.10.2",
    "rollup": "^0.55.1",
    "rollup-plugin-typescript": "^0.8.1",
    "typescript": "^2.6.2",
    "typescript-eslint-parser": "^12.0.0"
  },
  "jest": {
    "testMatch": ["**/src/**/*.test.ts"],
    "mapCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  },
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": true,
    "semi": false,
    "useTabs": false,
    "trailingComma": "all"
  }
}
